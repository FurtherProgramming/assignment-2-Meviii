<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="394.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.ForgotPasswordController">
    <children>
        <Label layoutX="70.0" layoutY="15.0" prefHeight="148.0" prefWidth="221.0" text="FORGOT PASSWORD&#10;" textAlignment="CENTER" textOverrun="CLIP">
            <font>
                <Font name="System Bold" size="22.0" />
            </font>
        </Label>
        <Button fx:id="btnForgPassToMainLogin" layoutX="315.0" layoutY="4.0" mnemonicParsing="false" onAction="#BackToMainLoginFromForgotPass" text="Back" />
      <TextField fx:id="txtForgPassUsername" layoutX="106.0" layoutY="151.0" promptText="USERNAME" />
      <Button fx:id="btnForgPassToChanPass" layoutX="153.0" layoutY="197.0" mnemonicParsing="false" onAction="#ForgPassToChanPass" text="Check" />
      <Label fx:id="txtIncorrectUsername" layoutX="128.0" layoutY="239.0" prefHeight="25.0" prefWidth="105.0" />
    </children>

</AnchorPane>
